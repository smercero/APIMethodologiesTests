<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>ListerImagesController</servlet-name>
    <servlet-class>Controllers.ListerImagesController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ListerImagesController</servlet-name>
    <url-pattern>/getImages</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>PutImagesController</servlet-name>
    <servlet-class>Controllers.PutImagesController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>PutImagesController</servlet-name>
    <url-pattern>/insertImages</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>DeleteImagesController</servlet-name>
    <servlet-class>Controllers.DeleteImagesController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DeleteImagesController</servlet-name>
    <url-pattern>/deleteImages</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>UpdateImagesController</servlet-name>
    <servlet-class>Controllers.UpdateImagesController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UpdateImagesController</servlet-name>
    <url-pattern>/updateImages</url-pattern>
  </servlet-mapping>

</web-app>
